{
  "name": "ml-extractor-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "ML Extractor Frontend - React + Tailwind CSS",
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
